<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true">
        <layer-list>
            <item
                android:drawable="@drawable/subbutton_clicked">
            </item>
        </layer-list>
    </item>
    <item android:state_enabled="false">
        <layer-list>
            <item
                android:drawable="@drawable/unenablebutton">
            </item>
        </layer-list>
    </item>
    <item android:state_pressed="false">
        <layer-list>
            <item
                android:drawable="@drawable/subbutton">
            </item>
        </layer-list>
    </item>

</selector>